{
	"name": "storeofbot",
	"main": "index.js",
	"preferGlobal": false,
	"version": "1.1.1",
	"description": "Source code of bot StoreOfBot on Telegram",
	"author": {
		"email": "TiagoDanin@outlook.com",
		"name": "Tiago Danin",
		"url": "https://TiagoDanin.github.io"
	},
	"license": "MIT",
	"keywords": [
		"bots",
		"store",
		"telegraf",
		"telegraf-bots",
		"telegram",
		"telegram-bot"
	],
	"scripts": {
		"start": "env-cmd node index.js",
		"dev": "env-cmd nodemon rs",
		"test": "xo",
		"i18n": "ls | grep .js | sort | npx nodejs-i18n --value | msgmerge --backup=off --update locales/pt.po /dev/stdin"
	},
	"engines": {
		"node": ">=9.9.0"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TiagoDanin/StoreOfBot.git"
	},
	"homepage": "https://TiagoDanin.github.io/Defend-The-Castle",
	"bugs": {
		"url": "https://github.com/TiagoDanin/StoreOfBot/issues"
	},
	"github": {
		"name": "StoreOfBot",
		"owner": "TiagoDanin"
	},
	"files": [
		"LICENSE",
		"README.md",
		"config.js",
		"database.js",
		"index.js",
		"locales",
		"package.json",
		"plugins"
	],
	"dependencies": {
		"debug": "^4.2.0",
		"json-stringify-safe": "5.0.1",
		"moment-timezone": "0.5.31",
		"nodejs-i18n": "2.4.0",
		"pg": "^8.3.3",
		"pg-native": "^3.0.0",
		"telegraf": "^3.38.0",
		"telegraf-start-parts": "^1.0.0"
	},
	"devDependencies": {
		"env-cmd": "10.1.0",
		"nodemon": "2.0.4",
		"xo": "^0.56.0"
	},
	"xo": {
		"rules": {
			"quote-props": "warn",
			"eqeqeq": "warn",
			"array-callback-return": "warn",
			"require-atomic-updates": "warn",
			"no-else-return": "off",
			"camelcase": "off",
			"no-useless-escape": "warn",
			"block-scoped-var": "off",
			"no-await-in-loop": "off"
		},
		"semicolon": false
	}
}
